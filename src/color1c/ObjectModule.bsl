//	    * v - Число от 0 до 100 - Value/Brightness.
//
// Возвращаемое значение:
//	Цвет - цвет.
//
Функция HSVВЦвет(Знач СтруктураHSV) Экспорт
    
    Возврат HSVВЦветПоКомпонентам(СтруктураHSV.h, СтруктураHSV.s, СтруктураHSV.v);
    
КонецФункции

// Конвертирует цвет в HSV-структуру.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
//
// Возвращаемое значение:
//  Cтруктура
//	    * h - Число от 0 до 360 - Hue.
//	    * s - Число от 0 до 100 - Saturation.
//	    * v - Число от 0 до 100 - Value/Brightness.
//
Функция ЦветВHSV(Знач Цвет) Экспорт
    
    Цвет = АбсолютныйЦвет(Цвет);

    r = Цвет.Красный;
    g = Цвет.Зеленый;
    b = Цвет.Синий;
			
	min = Мин(r, g, b);
	max = Макс(r, g, b);
	delta = max - min;
	h = 0;
	s = 0;
	v = max;

	v = Окр(max / 255 * 100);
	Если max <> 0 И delta <> 0 Тогда
	    s = Окр(delta / max * 100);
	Иначе
	    // black
	    Возврат Новый Структура("h, s, v", 0, 0, 0);
	КонецЕсли;

	Если( r = max ) Тогда
	    h = ( g - b ) / delta;         // between yellow & magenta
	ИначеЕсли ( g = max ) Тогда
	    h = 2 + ( b - r ) / delta;     // between cyan & yellow
	Иначе
	    h = 4 + ( r - g ) / delta;     // between magenta & cyan
	КонецЕсли;

	h = Окр(h * 60);            		// degrees
	Если ( h < 0 ) Тогда 
	    h = h + 360; 
	КонецЕсли;

    Возврат Новый Структура("h, s, v", h, s, v);
    
КонецФункции

