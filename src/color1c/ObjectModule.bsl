// Возвращает цвет из цветовой модели CMYK.
//
// Параметры:
//	c - Число от 0 до 100 - Cyan
//	m - Число от 0 до 100 - Magenta
//	y - Число от 0 до 100 - Yellow
//	k - Число от 0 до 100 - Key или Black
//
// Возвращаемое значение:
//	Цвет - цвет.
//
Функция CMYKВЦвет(Знач c, Знач m, Знач y, Знач k) Экспорт
    
    c = c / 100;
    m = m / 100;
    y = y / 100;
    k = k / 100;

    r = 1 - Мин(1, c * ( 1 - k ) + k);
    g = 1 - Мин(1, m * ( 1 - k ) + k);
    b = 1 - Мин(1, y * ( 1 - k ) + k);

    r = Окр(r * 255);
    g = Окр(g * 255);
    b = Окр(b * 255);
    
    Возврат Новый Цвет(r, g, b);

КонецФункции

// Цвет в структуру CMYK вид.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
// 
// Возвращаемое значение:
//  Структура - структура с цветом в CMYK.
//	    * c - Число от 0 до 100 - Cyan.
//	    * m - Число от 0 до 100 - Magenta.
//	    * y - Число от 0 до 100 - Yellow.
//	    * k - Число от 0 до 100 - Key или Black.
//
Функция ЦветВCMYK(Знач Цвет) Экспорт
    
    АбсолютныйЦвет = ЦветВHex(Цвет);
    
    result = Новый Структура();
    result.Вставить("c", 0);
    result.Вставить("m", 0);
    result.Вставить("y", 0);
    result.Вставить("k", 0);

    r = КрасныйИзHex(АбсолютныйЦвет) / 255;
    g = ЗеленыйИзHex(АбсолютныйЦвет) / 255;
    b = СинийИзHex(АбсолютныйЦвет) / 255;

    result.k = Мин( 1 - r, 1 - g, 1 - b );
	
	Если result.k = 1 Тогда
	    result.c = 0;
	    result.m = 0;
	    result.y = 0;
	Иначе
	    result.c = ( 1 - r - result.k ) / ( 1 - result.k );
	    result.m = ( 1 - g - result.k ) / ( 1 - result.k );
	    result.y = ( 1 - b - result.k ) / ( 1 - result.k );
		
	    result.c = Окр( result.c * 100 );
	    result.m = Окр( result.m * 100 );
	    result.y = Окр( result.y * 100 );
	    result.k = Окр( result.k * 100 );
	КонецЕсли;

    return result;    
    
КонецФункции

