		//ВременныйФайл = ПолучитьИмяВременногоФайла("htm");
		//ТабДокумент.Записать(ВременныйФайл, ТипФайлаТабличногоДокумента.HTML);
		//ТекстовыйДокумент = Новый ТекстовыйДокумент;
		//ТекстовыйДокумент.Прочитать(ВременныйФайл);
		//Стр = ТекстовыйДокумент.ПолучитьТекст();
		//УдалитьФайлы(ВременныйФайл);
		//
		//СтрокаПоиска = "background-color:";          
		//Позиция = СтрНайти(Стр, СтрокаПоиска);
		//Если Позиция <> 0 Тогда
		//    Результат = ВРег(Сред(Стр, Позиция + СтрДлина(СтрокаПоиска) + 2, 6));
		//КонецЕсли;
		//#КонецЕсли
		
		// Для версий платформ 8.3.9 и выше.
