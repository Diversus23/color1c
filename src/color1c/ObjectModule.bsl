// Функция переводит шестнадцатеричное число в десятичное
//
// Параметры: 
//	Число16 - Строка - HEX-число
//
// Возвращаемое значение:
//	Число - Десятичное число
//
Функция HexToDecЧисло(Знач Число16)
	
	Если Число16 = "0" Тогда 
		Возврат 0;
	КонецЕсли;
	
	Строка	 		= "0123456789ABCDEF";
	Число16 		= СокрЛП(Число16);
	СтаршаяСтепень 	= СтрДлина(Число16) - 1;
	Результат 		= 0;
	Индекс 			= 1;
	Основание		= 16;
	Пока СтаршаяСтепень >= 0 Цикл
		ТекущийСимвол 	= Сред(Число16, Индекс, 1);
		Представление 	= СтрНайти(Строка, ТекущийСимвол) - 1;
		Результат 		= Результат + Представление * Pow(Основание, СтаршаяСтепень);
		СтаршаяСтепень 	= СтаршаяСтепень - 1;
		Индекс	 		= Индекс + 1;
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции // HexToDec()   

// Конвертация из десятичной в любую систему счисления.
//
// Параметры:
//	Значение - Число - исходное значение.
//	Нотация - Число - основание системы счисления.
//
// Возвращаемое значение:
//	Строка - значение в нужной системе счисления.
//
Функция Из_10_В_Любую(Знач Значение = 0, Знач Нотация = 16)
	
	Если Нотация <= 0 Тогда 
		Возврат "";
	КонецЕсли;
	Значение = Число(Значение);
	Если Значение <= 0 Тогда 
		Возврат "0";
	КонецЕсли;
	
	Значение = Цел(Значение);
	Результат = "";
	Строка = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ"; 
	Пока Значение > 0 Цикл
		Результат = Сред(Строка, Значение % Нотация + 1, 1) + Результат;
		Значение = Цел(Значение / Нотация);		
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Вспомогательная функция перевода из системы HSL в RGB.
//
// Параметры:
//	ПервыйПараметр - Число - p 
//	ВторойПараметр - Число - q
//	Тон - Число - t тон
//
// Возвращаемое значение:
//	Число - промежуточное значение.
//
Функция ТонВЦвет(Знач ПервыйПараметр, Знач ВторойПараметр, Знач Тон)
	
	МаксимумЧастей	= 6;
	ДвеЧасти		= 2;
	ТриЧасти		= 3;
	
    Если Тон < 0 Тогда 
        Тон = Тон + 1; 
    КонецЕсли;
    Если Тон > 1 Тогда 
        Тон = Тон - 1;
    КонецЕсли;
    Если Тон < 1 / МаксимумЧастей Тогда 
        Возврат ПервыйПараметр + (ВторойПараметр - ПервыйПараметр) * МаксимумЧастей * Тон; 
    КонецЕсли;
    Если Тон < 1 / ДвеЧасти Тогда 
        Возврат ВторойПараметр; 
    КонецЕсли;
    Если Тон < ДвеЧасти / ТриЧасти Тогда 
        Возврат ПервыйПараметр + (ВторойПараметр - ПервыйПараметр) * (ДвеЧасти / ТриЧасти - Тон) * МаксимумЧастей; 
    КонецЕсли;
    
    Возврат ПервыйПараметр;
  
КонецФункции

// Возвращает случайное число. Работает как в веб-клиенте, так и в тонком.
// В веб-клиенте не использует ГенераторСлучайныхЧисел.
//
// Параметры:
//	МинимальноеЗначение - Число - минимальная граница случайного числа.
//	МаксимальноеЗначение - Число - максимальная граница случайного числа.
//
// Возвращаемое значение:
//	Число - случайное число в диапазоне переданных чисел (включая минимум и максимум).
//
Функция СлучайноеЧисло(Знач МинимальноеЗначение = 0, Знач МаксимальноеЗначение = 100)
	
	// Находим длину диапазона
	Альфа = МаксимальноеЗначение - МинимальноеЗначение;
	
	УИД = НРег(Новый УникальныйИдентификатор);
	Пока Истина Цикл
		УИД = СтрЗаменить(УИД,"-","");
		УИД = СтрЗаменить(УИД,"a","");
		УИД = СтрЗаменить(УИД,"b","");
		УИД = СтрЗаменить(УИД,"c","");
		УИД = СтрЗаменить(УИД,"d","");
		УИД = СтрЗаменить(УИД,"e","");
		УИД = СтрЗаменить(УИД,"f","");
		Если НЕ ПустаяСтрока(УИД) Тогда
			Прервать;
		Иначе
			УИД = НРег(Новый УникальныйИдентификатор);
		КонецЕсли;
	КонецЦикла;
	
	#Если ВебКлиент Тогда
		
	// Получаем диапазон от 0 до 1
	ЧислоУИД = Число("0." + УИД);
	// Проецируем диапазон от 0 до 1 на отрезок [МинимальноеЗначение, МаксимальноеЗначение]
	ЧислоУИД = МинимальноеЗначение + Окр(ЧислоУИД * Альфа);
	Если ЧислоУИД < МинимальноеЗначение Тогда
		ЧислоУИД = МинимальноеЗначение;	
	ИначеЕсли ЧислоУИД > МаксимальноеЗначение Тогда
		ЧислоУИД = МаксимальноеЗначение;	
	КонецЕсли;
	
	Возврат ЧислоУИД;
	
	#Иначе
	
	// Инициализируем по переданному числу, чтобы последовательные вызовы возвращали разные значения.
	ГСЧ = Новый ГенераторСлучайныхЧисел(Число(УИД));	
	Возврат ГСЧ.СлучайноеЧисло(МинимальноеЗначение, МаксимальноеЗначение);
	
	#КонецЕсли
	
КонецФункции

// Формирует строку повторяющихся символов заданной длины.
//
// Параметры:
//  Символ      - Строка - символ, из которого будет формироваться строка.
//  ДлинаСтроки - Число  - требуемая длина результирующей строки.
//
// Возвращаемое значение:
//  Строка - строка, состоящая из повторяющихся символов.
//
Функция СформироватьСтрокуСимволов(Знач Символ, Знач ДлинаСтроки) Экспорт
	
	Результат = "";
	Для Счетчик = 1 По ДлинаСтроки Цикл
		Результат = Результат + Символ;
	КонецЦикла;
	
	Возврат Результат;
	
КонецФункции

// Дополняет строку символами слева или справа до заданной длины и возвращает ее.
// При этом удаляются незначащие символы слева и справа (подробнее про незначащие символы 
// см. синтакс-помощник к методу платформы СокрЛП). 
// По умолчанию функция дополняет строку символами "0" (ноль) слева.
//
// Параметры:
//  Значение    - Строка - исходная строка, которую необходимо дополнить символами;
//  ДлинаСтроки - Число  - требуемая результирующая длина строки;
//  Символ      - Строка - символ, которым необходимо дополнить строку;
//  Режим       - Строка - "Слева" или "Справа" - вариант добавления символов к исходной строке.
// 
// Возвращаемое значение:
//  Строка - строка, дополненная символами.
//
// Пример:
//  1. Результат = ДополнитьСтроку("1234", 10, "0", "Слева");
//  Возвращает: "0000001234".
//
//  2. Результат = ДополнитьСтроку(" 1234  ", 10, "#", "Справа");
//  Строка = " 1234  "; ДлинаСтроки = 10; Символ = "#"; Режим = "Справа"
//  Возвращает: "1234######".
//
Функция ДополнитьСтроку(Знач Значение, Знач ДлинаСтроки, Знач Символ = "0", Знач Режим = "Слева")
	
	// Длина символа не должна превышать единицы.
	Символ = Лев(Символ, 1);
	
	// Удаляем крайние пробелы слева и справа строки.
	Значение = СокрЛП(Значение);
	КоличествоСимволовНадоДобавить = ДлинаСтроки - СтрДлина(Значение);
	
	Если КоличествоСимволовНадоДобавить > 0 Тогда
		
		СтрокаДляДобавления = СформироватьСтрокуСимволов(Символ, КоличествоСимволовНадоДобавить);
		Если ВРег(Режим) = "СЛЕВА" Тогда
			Значение = СтрокаДляДобавления + Значение;
		ИначеЕсли ВРег(Режим) = "СПРАВА" Тогда
			Значение = Значение + СтрокаДляДобавления;
		КонецЕсли;
		
	КонецЕсли;
	
	Возврат Значение;
	
КонецФункции

// Возвращает числовой вес версии для сравнения версий между собой
//
// Параметры:
//  Версия - Строка - Версия в строковом формате
//
// Возвращаемое значение:
//  Число - вес версии
//
Функция ВесВерсии(Знач Версия)
	
	Если Версия = "" Тогда
		Возврат 0;
	КонецЕсли;
	
	Массив = СтрРазделить(Версия, ".");
	ПерваяВерсия = 1000000000;
	ВтораяВерсия = 1000000;
	ТретьяВерсия = 1000;
	
	Возврат	Число(Массив[0]) * ПерваяВерсия
		+ Число(Массив[1]) * ВтораяВерсия
		+ Число(Массив[2]) * ТретьяВерсия
		+ Число(Массив[3]);	
	
	КонецФункции
	
