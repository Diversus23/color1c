////////////////////////////////////////////////////////////////////////////////
//
// Модуль для работы с цветом в 1С.
// https://github.com/Diversus23/color1c
// Версия: 1.5.2
//
// Модуль использует HEX-представления цвета вида #RRGGBB
// где RR - красный, GG - зеленый, BB - синий в HEX-представлении.
// Хранить элементы цвета с справочниках рекомендуем с типом Строка[7].
// Пример, красный цвет: #FF0000 или #F00
//
// Модуль определяет работу с:
//  + HEX-представлением цвета (конвертация из HEX в цвет 1С и наоборот);
//  + HSV-представлением цвета;
//  + HSL-представлением цвета;
//  + CMYK-представлением цвета;
//  + Конвертациями из одной системы представления цвета в другую;
//  + Работа с каталогами RAL;
//  + Работа со случайными цветами;
//		+ Получение случайного цвета;
//		+ Получение случайного светлого цвета;
//		+ Получение случайного темного цвета;
//		+ Получение случайного цвета из каталога RAL Standart;
//		+ Получение случайного Web-цвета;
//  + Получение контрастных и инвертированных цветов для текста и фона;
//  + Извлечение компонент цвета (красный, зеленый, синий).
//  + Трансформация цветов:
//		+ осветление;
//		+ затемнение;
//		+ различные алгоритмы градации серого;
//		+ смешивание двух цветов;
//	+ Работа с именованными CSS-цветами (black, green, white...)
//
// Использовался инструмент https://colorscheme.ru/color-converter.html
//
////////////////////////////////////////////////////////////////////////////////

#Область ПрограммныйИнтерфейс

#Область HEX
	
// Функция - Цвет в HEX-вид.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
// 
// Возвращаемое значение:
//  Строка - строка вида #RRGGBB - где RR - красный, GG - зеленый, BB - синий.
//
Функция ЦветВHex(Знач Цвет) Экспорт
		
	Результат = "";
		
	Если Цвет.Вид = ВидЦвета.Абсолютный Тогда
		
		Результат = ДополнитьСтроку(Из_10_В_Любую(Цвет.Красный), 2)
			+ ДополнитьСтроку(Из_10_В_Любую(Цвет.Зеленый), 2)
			+ ДополнитьСтроку(Из_10_В_Любую(Цвет.Синий), 2);
			
	ИначеЕсли Цвет.Вид = ВидЦвета.WebЦвет Тогда
		
		НайденныйЦвет = КаталогWebЦветов().Получить(Цвет);               
		Если НайденныйЦвет <> Неопределено Тогда
			Возврат ЦветВHex(НайденныйЦвет); 
		Иначе
			Результат = "000000";
		КонецЕсли;                              
		
	ИначеЕсли Цвет.Вид = ВидЦвета.WindowsЦвет ИЛИ Цвет.Вид = ВидЦвета.ЭлементСтиля Тогда

        Результат = "000000";
			
		// Для версий платформ 8.3.9 и выше.
		СистемнаяИнформация = Новый СистемнаяИнформация;
		ВесПлатформы = ВесВерсии(СистемнаяИнформация.ВерсияПриложения);
		Вес = ВесВерсии("8.3.9.0");
		Если ВесПлатформы >= Вес Тогда
			ПотокВПамяти = Новый ПотокВПамяти();

			ТабДокумент = Новый ТабличныйДокумент;
			ТабДокумент.Область(1, 1, 1, 1).ЦветФона = Цвет;
			ТабДокумент.Записать(ПотокВПамяти, ТипФайлаТабличногоДокумента.ODS);

			ПотокВПамяти.Перейти(0, ПозицияВПотоке.Начало);

			ТабДокумент.Прочитать(ПотокВПамяти,, ТипФайлаТабличногоДокумента.ODS);
			ЦветФона = ТабДокумент.Область(1, 1, 1, 1).ЦветФона;

			Результат = ДополнитьСтроку(Из_10_В_Любую(ЦветФона.Красный), 2)
			    + ДополнитьСтроку(Из_10_В_Любую(ЦветФона.Зеленый), 2)
			    + ДополнитьСтроку(Из_10_В_Любую(ЦветФона.Синий), 2);
			    
			ПотокВПамяти.Закрыть();
		Иначе
			#Если НЕ ВебКлиент Тогда
				
			ТабДокумент = Новый ТабличныйДокумент;
			ТабДокумент.Область(1, 1, 1, 1).ЦветФона = Цвет;
			ВременныйФайл = ПолучитьИмяВременногоФайла("htm");
			ТабДокумент.Записать(ВременныйФайл, ТипФайлаТабличногоДокумента.HTML);
			ТекстовыйДокумент = Новый ТекстовыйДокумент;
			ТекстовыйДокумент.Прочитать(ВременныйФайл);
			Стр = ТекстовыйДокумент.ПолучитьТекст();
			УдалитьФайлы(ВременныйФайл);

			ДлинаСтрокиЦвета = 6;
			ДлинаОтступа = 2;
			СтрокаПоиска = "background-color:";          
			Позиция = СтрНайти(Стр, СтрокаПоиска);
			Если Позиция <> 0 Тогда
				Результат = ВРег(Сред(Стр, Позиция + СтрДлина(СтрокаПоиска) + ДлинаОтступа, ДлинаСтрокиЦвета));
			КонецЕсли;
			
			#КонецЕсли			
		КонецЕсли;
		
	Иначе
		
		Результат = "000000";
		
	КонецЕсли;
	
	Возврат "#" + Результат;
	
КонецФункции

// Возвращает из строки в HEX-виде ее цвет.
//
// Параметры:
//	Стр - Строка - вида #RRGGBB или #RGB - где R, RR - красный, G, GG - зеленый, B, BB - синий, 
//
// Возвращаемое значение:
//	Цвет - цвет.
//
Функция HexВЦвет(Знач Стр) Экспорт
	
	ДлинаСокращенногоЦвета 	= 4;
	ДлинаПолногоЦвета 		= 7;
	
    СтрЦвета = ВРег(Стр);
    ЦветОпределен = Истина;
	
	Попытка
		Если СтрДлина(СтрЦвета) = ДлинаСокращенногоЦвета Тогда
			// Цвет вида #RGB -> #RRGGBB (#F18 -> #FF1188).
			СтрЦвета = Лев(СтрЦвета, 1)
				+ Сред(СтрЦвета, 2, 1) + Сред(СтрЦвета, 2, 1)
				+ Сред(СтрЦвета, 3, 1) + Сред(СтрЦвета, 3, 1)
				+ Сред(СтрЦвета, 4, 1) + Сред(СтрЦвета, 4, 1);
		КонецЕсли;
		
		Если СтрДлина(СтрЦвета) = ДлинаПолногоЦвета Тогда
			// Цвет вида #RRGGBB (#FF1188 - FF - красный, 11 - зеленый, 88 - синий).
			Красный 		= HexToDecЧисло(Сред(СтрЦвета, 2, 2));
			Зеленый 		= HexToDecЧисло(Сред(СтрЦвета, 4, 2));
			Синий 			= HexToDecЧисло(Сред(СтрЦвета, 6, 2));
		Иначе
			ЦветОпределен	= Ложь;
		КонецЕсли;
	Исключение
		ЦветОпределен = Ложь;
	КонецПопытки;
	
	Если НЕ ЦветОпределен Тогда
		МаксимумЦвет	= 255;
		Красный 		= МаксимумЦвет;
		Зеленый 		= МаксимумЦвет;
		Синий 			= МаксимумЦвет;		
	КонецЕсли;
	
	Возврат КонструкторЦвета(Красный, Зеленый, Синий);
	
КонецФункции

#КонецОбласти

#Область ТрансформацияЦветов

// Функция "осветляет" цвет.
//
// Параметры:
//  Цвет	 - Цвет	 - исходный цвет
//  Процент	 - Число - процент изменения светлоты цвета. Значение 0 цвет не изменен, 100 - белый.
// 
// Возвращаемое значение:
//  Цвет - осветленный цвет.
//
Функция ОсветлитьЦвет(Знач Цвет, Знач Процент) Экспорт
	
	МаксимумЦвет	= 255;
	МаксимумПроцент	= 100;
	
    Цвет 	= АбсолютныйЦвет(Цвет);    
    Дельта 	= Макс(Мин(Процент, МаксимумЦвет), 0) / МаксимумПроцент;
	
	Красный = Мин(Цвет.Красный + Дельта * (МаксимумЦвет - Цвет.Красный), МаксимумЦвет);
	Зеленый	= Мин(Цвет.Зеленый + Дельта * (МаксимумЦвет - Цвет.Зеленый), МаксимумЦвет);
	Синий	= Мин(Цвет.Синий   + Дельта * (МаксимумЦвет - Цвет.Синий), МаксимумЦвет);
	
	Возврат КонструкторЦвета(Окр(Красный), Окр(Зеленый), Окр(Синий));
	
КонецФункции

// Функция "затемняет" цвет.
//
// Параметры:
//  Цвет	 - Цвет	 - исходный цвет
//  Процент	 - Число - процент изменения затемнения цвета. Значение 0 цвет не изменен, 100 - черный.
// 
// Возвращаемое значение:
//  Цвет - затемненный цвет.
//
Функция ЗатемнитьЦвет(Знач Цвет, Знач Процент) Экспорт
    
	МаксимумЦвет	= 255;
	МаксимумПроцент	= 100;

	Цвет = АбсолютныйЦвет(Цвет);    
    Дельта = Макс(Мин(Процент, МаксимумЦвет), 0) / МаксимумПроцент;
	
	Красный	= Макс(Цвет.Красный - Дельта * Цвет.Красный, 0);
	Зеленый	= Макс(Цвет.Зеленый - Дельта * Цвет.Зеленый, 0);
	Синий	= Макс(Цвет.Синий   - Дельта * Цвет.Синий, 0);
	
	Возврат КонструкторЦвета(Окр(Красный), Окр(Зеленый), Окр(Синий));    
    
КонецФункции

// Смешивание двух цветов с балансом.
//
// Параметры:
//	Цвет1 - Цвет, Строка - первый цвет.
//	Цвет2 - Цвет, Строка - второй цвет.
//  Баланс - Число - баланс смешивания двух цветов (от 0 до 100).
//
// Возвращаемое значение:
//	Цвет - цвет получившийся при смешивании.
//      Примеры:
//          (#38c120, #3ab5ba, 50) = #39BB6D
//          (#38c120, #3ab5ba, 80) = #3AB79B
//
//  Онлайн пример: https://pinetools.com/blend-colors но там небольшая ошибка с округлениями.
//
Функция СмешатьДваЦвета(Знач Цвет1, Знач Цвет2, Знач Баланс = 50) Экспорт
	
	МаксимумЦвет	= 255;
	МаксимумПроцент	= 100;
	
    Если Баланс > МаксимумПроцент Тогда
        Баланс = МаксимумПроцент;
    КонецЕсли;
    Если Баланс < 0 Тогда
        Баланс = 0;
    КонецЕсли;
    Баланс100 = Баланс / МаксимумПроцент;
    
    Ц1 = АбсолютныйЦвет(Цвет1);
    Ц2 = АбсолютныйЦвет(Цвет2);
    
    Красный = Макс(Мин(Окр((Ц2.Красный - Ц1.Красный) * Баланс100 + Ц1.Красный), МаксимумЦвет), 0);
    Зеленый = Макс(Мин(Окр((Ц2.Зеленый - Ц1.Зеленый) * Баланс100 + Ц1.Зеленый), МаксимумЦвет), 0);
    Синий   = Макс(Мин(Окр((Ц2.Синий - Ц1.Синий) * Баланс100 + Ц1.Синий), МаксимумЦвет), 0);
    
    Возврат КонструкторЦвета(Красный, Зеленый, Синий);
    
КонецФункции

// Конвертация цвета в градации серого по среднему методу.
//
// Параметры:
//	Цвет - Цвет, Строка - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет получившийся при градации при смешивании всех компонентов цвета и получении среднего.
//
//  Формула: (R + G + B) / 3
//  Подробнее на: https://www.johndcook.com/blog/2009/08/24/algorithms-convert-color-grayscale/
//
Функция ГрадацииСерогоAverage(Знач Цвет) Экспорт
	
	КомпонентыЦвета = 3;	
    Цвет  = АбсолютныйЦвет(Цвет);
    Серый = (Цвет.Красный + Цвет.Зеленый + Цвет.Синий) / КомпонентыЦвета;
    
    Возврат КонструкторЦвета(Серый, Серый, Серый);
    
КонецФункции

// Конвертация цвета в градации серого по осветленному методу. Метод имеет тенденцию уменьшать контраст.
//
// Параметры:
//	Цвет - Цвет, Строка - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет получившийся при градации при осветлении.
//
//  Формула: (max (R, G, B) + min (R, G, B)) / 2
//  Подробнее на: https://www.johndcook.com/blog/2009/08/24/algorithms-convert-color-grayscale/
//
Функция ГрадацииСерогоLightness(Знач Цвет) Экспорт
    
    Цвет  = АбсолютныйЦвет(Цвет);
    Серый = (Макс(Цвет.Красный, Цвет.Зеленый, Цвет.Синий) + Мин(Цвет.Красный, Цвет.Зеленый, Цвет.Синий)) / 2;
    
    Возврат КонструкторЦвета(Серый, Серый, Серый);
    
КонецФункции

// Конвертация цвета в градации серого по методу светимости. Рекомендуется по умолчанию. Используется в редакторе GIMP.
//
// Параметры:
//	Цвет - Цвет, Строка - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет получившийся при градации при светимости.
//
//  Формула: 0,21 * Red + 0,72 * Green + 0,07 * Blue
//  Этот метод работает лучше всего в целом и является методом по умолчанию, используемым в редакторе GIMP.
//  Подробнее на: https://www.johndcook.com/blog/2009/08/24/algorithms-convert-color-grayscale/
//
Функция ГрадацииСерогоLuminosity(Знач Цвет) Экспорт
	
	ЧислоКрасный	= 0.21;
	ЧислоЗеленый	= 0.72;
	ЧислоСиний		= 0.07;
	
    Цвет  = АбсолютныйЦвет(Цвет);
    Серый = ЧислоКрасный * Цвет.Красный + ЧислоЗеленый * Цвет.Зеленый + ЧислоСиний * Цвет.Синий;
    
    Возврат КонструкторЦвета(Серый, Серый, Серый);
    
КонецФункции

// Приводит цвет к абсолютному (ВидЦвета.Абсолютный)
//
// Параметры:
//	Цвет - Цвет, Строка - произвольный цвет, либо hex-цвет.
//
// Возвращаемое значение:
//	Цвет - цвет результат с видом ВидЦвета.Абсолютный.
//
Функция АбсолютныйЦвет(Знач Цвет) Экспорт
    
	Если ТипЗнч(Цвет) = Тип("Цвет") Тогда
		ИсходныйЦвет = Цвет;
	ИначеЕсли ТипЗнч(Цвет) = Тип("Строка") Тогда
		ИсходныйЦвет = HexВЦвет(Цвет);
	Иначе
		Возврат КонструкторЦвета(0, 0, 0);
	КонецЕсли;
	
	// Преобразования для не абсолютных цветов.
	Если ИсходныйЦвет.Вид <> ВидЦвета.Абсолютный Тогда
		ИсходныйЦвет = HexВЦвет(ЦветВHex(ИсходныйЦвет));
	КонецЕсли;
    
    Возврат ИсходныйЦвет;
    
КонецФункции

// Возвращает массив цветов, представляющих собой градиент - переход от начального цвета к конечному. 
// Возвращаемый массив начинается с начального и заканчивается конечным цветом.
// Автор: https://infostart.ru/public/978835/
//
// Параметры:
//	НачальныйЦвет - Цвет - начальный цвет градиента
//	КонечныйЦвет - Цвет - конечный цвет градиента
//	КоличествоГрадаций - Число - общее число цветов в возвращаемом массиве (включает начальный и конечный цвета), должно быть больше 2. По умолчанию 5.
//
// Возвращаемое значение:
//	Массив,Неопределено - массив с элементами типа Цвет или Неопределено (если градаций меньше 3 либо начальный и конечный цвета
//								заданы не абсолютными цветами
//
Функция ПолучитьГрадиент(Знач НачальныйЦвет, Знач КонечныйЦвет, Знач КоличествоГрадаций = 5) Экспорт
	
	мНачальныйЦвет = АбсолютныйЦвет(НачальныйЦвет);
	мКонечныйЦвет = АбсолютныйЦвет(КонечныйЦвет);
	
	МассивЦветов = Новый Массив;
	МассивЦветов.Добавить(НачальныйЦвет);
	
	Если КоличествоГрадаций > 2 Тогда
		ШагКрасного = (мКонечныйЦвет.Красный - мНачальныйЦвет.Красный) / КоличествоГрадаций;
		ШагЗеленого = (мКонечныйЦвет.Зеленый - мНачальныйЦвет.Зеленый) / КоличествоГрадаций;
		ШагСинего   = (мКонечныйЦвет.Синий - мНачальныйЦвет.Синий) / КоличествоГрадаций;
		
		ТекКрасный  = мНачальныйЦвет.Красный;
		ТекЗеленый  = мНачальныйЦвет.Зеленый;
		ТекСиний    = мНачальныйЦвет.Синий;
		
		Для Сч = 2 По КоличествоГрадаций Цикл
			ТекКрасный  = ТекКрасный + ШагКрасного;
			ТекЗеленый  = ТекЗеленый + ШагЗеленого;
			ТекСиний    = ТекСиний + ШагСинего;
			
			МассивЦветов.Добавить(КонструкторЦвета(Окр(ТекКрасный, 0), Окр(ТекЗеленый, 0), Окр(ТекСиний, 0)));
		КонецЦикла;
		
		МассивЦветов[МассивЦветов.ВГраница()] = мКонечныйЦвет;
		
		Возврат МассивЦветов;
		
	Иначе
		
		Возврат Неопределено;
		
	КонецЕсли;
	
КонецФункции

// Возвращает из строки в HEX-виде ее красную составляющую.
//
// Параметры:
//	СтрокаЦвета - Строка - вида #RRGGBB - где RR - красный, GG - зеленый, BB - синий
//
// Возвращаемое значение:
//	Число - составляющая.
//
Функция КрасныйИзHex(Знач СтрокаЦвета) Экспорт
	
	Цвет = HexВЦвет(СтрокаЦвета);
	Возврат Цвет.Красный;
	
КонецФункции

