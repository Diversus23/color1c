// Возвращает цвет из цветовой модели CMYK.
//
// Параметры:
//	Голубой - Число - Cyan от 0 до 100
//	Пурпурный - Число - Magenta от 0 до 100
//	Желтый - Число - Yellow от 0 до 100
//	Черный - Число - Key или Black от 0 до 100
//
// Возвращаемое значение:
//	Цвет - цвет.
//
Функция CMYKВЦвет(Знач Голубой, Знач Пурпурный, Знач Желтый, Знач Черный) Экспорт
	
	МаксимумПроцент	= 100;
	МаксимумЦвет	= 255;
	
    Голубой 	= Голубой / МаксимумПроцент;
    Пурпурный 	= Пурпурный / МаксимумПроцент;
    Желтый 		= Желтый / МаксимумПроцент;
    Черный 		= Черный / МаксимумПроцент;

    Красный = 1 - Мин(1, Голубой * ( 1 - Черный ) + Черный);
    Зеленый = 1 - Мин(1, Пурпурный * ( 1 - Черный ) + Черный);
    Синий 	= 1 - Мин(1, Желтый * ( 1 - Черный ) + Черный);

    Красный = Окр(Красный * МаксимумЦвет);
    Зеленый = Окр(Зеленый * МаксимумЦвет);
    Синий 	= Окр(Синий * МаксимумЦвет);
    
    Возврат КонструкторЦвета(Красный, Зеленый, Синий);

КонецФункции

// Цвет в структуру CMYK вид.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
// 
// Возвращаемое значение:
//  Структура - структура с цветом в CMYK.
//	    * c - Число - Cyan от 0 до 100.
//	    * m - Число - Magenta от 0 до 100.
//	    * y - Число - Yellow от 0 до 100.
//	    * k - Число - Key или Black от 0 до 100.
//
Функция ЦветВCMYK(Знач Цвет) Экспорт
	
	МаксимумПроцент	= 100;
	МаксимумЦвет	= 255;
	
    АбсолютныйЦвет = ЦветВHex(Цвет);    
    Результат = Новый Структура();
    Результат.Вставить("c", 0);
    Результат.Вставить("m", 0);
    Результат.Вставить("y", 0);
    Результат.Вставить("k", 0);

    Красный = КрасныйИзHex(АбсолютныйЦвет) / МаксимумЦвет;
    Зеленый = ЗеленыйИзHex(АбсолютныйЦвет) / МаксимумЦвет;
    Синий	= СинийИзHex(АбсолютныйЦвет) / МаксимумЦвет;

    Результат.k = Мин( 1 - Красный, 1 - Зеленый, 1 - Синий );
	
	Если Результат.k = 1 Тогда
	    Результат.c = 0;
	    Результат.m = 0;
	    Результат.y = 0;
	Иначе
	    Результат.c = ( 1 - Красный - Результат.k ) / ( 1 - Результат.k );
	    Результат.m = ( 1 - Зеленый - Результат.k ) / ( 1 - Результат.k );
	    Результат.y = ( 1 - Синий - Результат.k ) / ( 1 - Результат.k );
		
	    Результат.c = Окр( Результат.c * МаксимумПроцент );
	    Результат.m = Окр( Результат.m * МаксимумПроцент );
	    Результат.y = Окр( Результат.y * МаксимумПроцент );
	    Результат.k = Окр( Результат.k * МаксимумПроцент );
	КонецЕсли;

    Возврат Результат;    
    
КонецФункции

