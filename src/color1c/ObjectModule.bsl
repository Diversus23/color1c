//	Число - составляющая.
//
Функция ЗеленыйИзHex(Знач СтрокаЦвета) Экспорт
	
	Цвет = HexВЦвет(СтрокаЦвета);
	Возврат Цвет.Зеленый;
	
КонецФункции

// Возвращает из строки в HEX-виде ее синюю составляющую.
//
// Параметры:
//	СтрокаЦвета - Строка - вида #RRGGBB - где RR - красный, GG - зеленый, BB - синий
//
// Возвращаемое значение:
//	Число - составляющая.
//
Функция СинийИзHex(Знач СтрокаЦвета) Экспорт
	
	Цвет = HexВЦвет(СтрокаЦвета);
	Возврат Цвет.Синий;
	
КонецФункции

// По переданному цвету возвращает контрастный белый или черный цвет.
// Позволяет получать для цвета фона цвет текста.
// Можно пользоваться в связке цвет фона получить из СлучайныйЦвет(),
// а цвет текста используя эту функцию. Позволяет подбирать фон для для шрифта.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет контраста.
// Пример: для цвета #FFFFFF вернет #000000
//
Функция КонтрастныйЦвет(Знач Цвет) Экспорт

	МаксимумЦвет 	= 255;
	ЧислоКрасный	= 0.299;
	ЧислоЗеленый	= 0.587;
	ЧислоСиний		= 0.114;
	ПоловинаЕдиницы	= 0.5;
	
    ИсходныйЦвет 	= АбсолютныйЦвет(Цвет);	
	Красный 		= ИсходныйЦвет.Красный;
	Зеленый 		= ИсходныйЦвет.Зеленый;
	Синий 			= ИсходныйЦвет.Синий;
		
 	Цветность 		= 1 - (ЧислоКрасный * Красный + ЧислоЗеленый * Зеленый + ЧислоСиний * Синий) / МаксимумЦвет;
	ЦветРезультата 	= ?(Цветность < ПоловинаЕдиницы, 0, МаксимумЦвет);
  	
  	Возврат КонструкторЦвета(ЦветРезультата, ЦветРезультата, ЦветРезультата);
	
КонецФункции

// По переданному цвету возвращает инвертированный (противоположный) цвет.
// Позволяет получать для цвета фона цвет текста.
// Можно пользоваться в связке цвет фона получить из СлучайныйЦвет(),
// а цвет текста используя эту функцию.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет контраста.
// Пример: для цвета #5200A3 вернет #ADFF5C
//
Функция ИнвертированныйЦвет(Знач Цвет) Экспорт
    
    ИсходныйЦвет 	= АбсолютныйЦвет(Цвет);
	
	Красный 		= ИсходныйЦвет.Красный;
	Зеленый 		= ИсходныйЦвет.Зеленый;
	Синий 			= ИсходныйЦвет.Синий;
	
	МаксимумЦвет 	= 255;
	  	
  	Возврат КонструкторЦвета(МаксимумЦвет - Красный, МаксимумЦвет - Зеленый, МаксимумЦвет - Синий);
	
КонецФункции

// Выполняет Hue Shift. Сдвиг компоненты Hue в цвете на нужное количество градусов.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//  Оттенок - Число - градус сдвига. Имеет смысл -180 до 180, если не в диапазоне будет к нему приведен.
//
// Возвращаемое значение:
//	Цвет - цвет с Hue Shift.
//
Функция СдвигОттенкаЦвета(Знач Цвет, Знач Оттенок = 0) Экспорт
	
	МаксимумГрадусов = 360;
	
    ИсходныйЦвет = ЦветВHSV(Цвет);
    ИсходныйЦвет.h = ИсходныйЦвет.h + Оттенок;
    Пока ИсходныйЦвет.h >= МаксимумГрадусов Цикл
        ИсходныйЦвет.h = ИсходныйЦвет.h - МаксимумГрадусов;
    КонецЦикла;
    Пока ИсходныйЦвет.h < 0 Цикл 
        ИсходныйЦвет.h = ИсходныйЦвет.h + МаксимумГрадусов;
    КонецЦикла;
    
    Возврат HSVВЦвет(ИсходныйЦвет);
        
КонецФункции

