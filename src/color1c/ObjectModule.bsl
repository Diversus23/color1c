// По переданному цвету возвращает контрастный белый или черный цвет.
// Позволяет получать для цвета фона цвет текста.
// Можно пользоваться в связке цвет фона получить из СлучайныйЦвет(),
// а цвет текста используя эту функцию.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет контраста.
// Пример: для цвета #FFFFFF вернет #000000
//
Функция КонтрастныйЦвет(Знач Цвет) Экспорт
	    
    ИсходныйЦвет = АбсолютныйЦвет(Цвет);
	
	Красный = ИсходныйЦвет.Красный;
	Зеленый = ИсходныйЦвет.Зеленый;
	Синий 	= ИсходныйЦвет.Синий;
	
 	Цветность 		= 1 - (0.299 * Красный + 0.587 * Зеленый + 0.114 * Синий) / 255;
	ЦветРезультата 	= ?(Цветность < 0.5, 0, 255);
  	
  	Возврат Новый Цвет(ЦветРезультата, ЦветРезультата, ЦветРезультата);
	
КонецФункции

// По переданному цвету возвращает инвертированный (противоположный) цвет.
// Позволяет получать для цвета фона цвет текста.
// Можно пользоваться в связке цвет фона получить из СлучайныйЦвет(),
// а цвет текста используя эту функцию.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//
// Возвращаемое значение:
//	Цвет - цвет контраста.
// Пример: для цвета #5200A3 вернет #ADFF5C
//
Функция ИнвертированныйЦвет(Знач Цвет) Экспорт
    
    ИсходныйЦвет = АбсолютныйЦвет(Цвет);
	
	Красный = ИсходныйЦвет.Красный;
	Зеленый = ИсходныйЦвет.Зеленый;
	Синий 	= ИсходныйЦвет.Синий;
	  	
  	Возврат Новый Цвет(255 - Красный, 255 - Зеленый, 255 - Синий);
	
КонецФункции

// Выполняет Hue Shift. Свдиг компоненты Hue в цвете на нужное количество градусов.
//
// Параметры:
//	Цвет - Цвет - исходный цвет.
//  Оттенок - Число - градус сдвига. Имеет смысл -180 до 180, если не в диапазоне будет к нему приведен.
//
// Возвращаемое значение:
//	Цвет - цвет с Hue Shift.
//
Функция СдвигОттенкаЦвета(Знач Цвет, Знач Оттенок = 0) Экспорт
    
    hsv = ЦветВHSV(Цвет);
    hsv.h = hsv.h + Оттенок;
    Пока hsv.h >= 360 Цикл
        hsv.h = hsv.h - 360;
    КонецЦикла;
    Пока hsv.h < 0 Цикл 
        hsv.h = hsv.h + 360;
    КонецЦикла;
    
    Возврат HSVВЦвет(hsv);
        
КонецФункции

