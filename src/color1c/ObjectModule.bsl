// Конвертирует цвет в HSL-структуру.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
//
// Возвращаемое значение:
//  Cтруктура
//	    * h - Число от 0 до 360 - Hue.
//	    * s - Число от 0 до 100 - Saturation.
//	    * l - Число от 0 до 100 - lightness.
//
Функция ЦветВHSL(Знач Цвет) Экспорт
    
    Цвет = АбсолютныйЦвет(Цвет);

    r = Цвет.Красный / 255;
    g = Цвет.Зеленый / 255;
    b = Цвет.Синий / 255;
    
    МаксЗначение = Макс(r, g, b);
    МинЗначение = Мин(r, g, b);
    h = 0;
    s = 0;
    l = (МаксЗначение + МинЗначение) / 2;

    Если (МаксЗначение = МинЗначение) Тогда
        h = 0;
        s = 0; // achromatic
    Иначе
        d = МаксЗначение - МинЗначение;
        s = ?(l > 0.5, d / (2 - МаксЗначение - МинЗначение), d / (МаксЗначение + МинЗначение));
        Если (МаксЗначение = r) Тогда
            h = (g - b) / d + ?(g < b, 6, 0);
        ИначеЕсли МаксЗначение = g Тогда
            h = (b - r) / d + 2;
        Иначе
            h = (r - g) / d + 4;
        КонецЕсли;
        
        h = h / 6;
    КонецЕсли;

    Возврат Новый Структура("h, s, l", 
        Окр(h * 360), 
        Окр(s * 100),
        Окр(l * 100));
    
КонецФункции

// Конвертирует HSL-структура в абсолютный цвет.
//
// Параметры:
//  СтруктураHSL - исходная структура с представлением цвета.
//	    * h - Число от 0 до 360 - Hue.
//	    * s - Число от 0 до 100 - Saturation.
//	    * l - Число от 0 до 100 - Lightness.
//
// Возвращаемое значение:
//	Цвет - абсолютный цвет.
//
Функция HSLВЦвет(Знач СтруктураHSL) Экспорт
    
    h = СтруктураHSL.h;
    s = СтруктураHSL.s;
    l = СтруктураHSL.l;
    
    r = 0;
    g = 0;
    b = 0;
    
    Если s = 0 Тогда
        r = l;
        g = l;
        b = l; // achromatic
    Иначе
        
        q = ?(l < 0.5, l * (1 + s), l + s - l * s);
        p = 2 * l - q;
        
        r = HUE2RGB(p, q, h + 1/3);
        g = HUE2RGB(p, q, h);
        b = HUE2RGB(p, q, h - 1/3);
        
    КонецЕсли;
    
    Возврат Новый Цвет(r * 255, g * 255, b * 255);
    
КонецФункции

// Конвертирует HSL-структура в цвет.
//
// Параметры:
//  СтруктураHSL - исходная структура с представлением цвета.
//	    * h - Число от 0 до 360 - Hue.
//	    * s - Число от 0 до 100 - Saturation.
//	    * l - Число от 0 до 100 - Lightness.
//
// Возвращаемое значение:
