Функция HSVВЦвет(Знач СтруктураHSV) Экспорт
    
    Возврат HSVВЦветПоКомпонентам(СтруктураHSV.h, СтруктураHSV.s, СтруктураHSV.v);
    
КонецФункции

// Конвертирует цвет в HSV-структуру.
//
// Параметры:
//  Цвет - Цвет	 - исходный цвет.
//
// Возвращаемое значение:
//  Структура
//	    * h - Число - Hue от 0 до 360.
//	    * s - Число - Saturation от 0 до 100.
//	    * v - Число - Value/Brightness от 0 до 100.
//
Функция ЦветВHSV(Знач Цвет) Экспорт

	МаксимумПроцент	= 100;
	МаксимумЦвет	= 255;
	МаксимумГрадус	= 360;
	Градус60		= 60;
	Тон2			= 2;
	Тон4			= 4;
	
    Цвет = АбсолютныйЦвет(Цвет);
    Красный = Цвет.Красный;
    Зеленый = Цвет.Зеленый;
    Синий 	= Цвет.Синий;
			
	Минимум 		= Мин(Красный, Зеленый, Синий);
	Максимум 		= Макс(Красный, Зеленый, Синий);
	Дельта 			= Максимум - Минимум;
	Тон 			= 0;
	Насыщенность	= 0;
	Яркость			= Максимум;
	
	Яркость = Окр(Максимум / МаксимумЦвет * МаксимумПроцент);
	Если Максимум <> 0 И Дельта <> 0 Тогда
	    Насыщенность = Окр(Дельта / Максимум * МаксимумПроцент);
	Иначе
	    // black
	    Возврат Новый Структура("h, s, v", 0, 0, Яркость);
	КонецЕсли;

	Если Красный = Максимум Тогда
	    Тон = (Зеленый - Синий) / Дельта;     		// between yellow & magenta
	ИначеЕсли Зеленый = Максимум Тогда
	    Тон = Тон2 + (Синий - Красный) / Дельта; 	// between cyan & yellow
	Иначе
	    Тон = Тон4 + (Красный - Зеленый) / Дельта;	// between magenta & cyan
	КонецЕсли;

	Тон = Окр(Тон * Градус60); // degrees
	Если Тон < 0 Тогда 
	    Тон = Тон + МаксимумГрадус; 
	КонецЕсли;

    Возврат Новый Структура("h, s, v", Тон, Насыщенность, Яркость);
    
КонецФункции

